<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>NewServlet</servlet-name>
        <servlet-class>com.bmb.controller.NewServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarTipo</servlet-name>
        <servlet-class>com.bmb.servlet.CadastrarTipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarTipo</servlet-name>
        <servlet-class>com.bmb.servlet.EditarTipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeletarTipo</servlet-name>
        <servlet-class>com.bmb.servlet.DeletarTipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarTipo</servlet-name>
        <servlet-class>com.bmb.servlet.ListarTipo</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarProduto</servlet-name>
        <servlet-class>com.bmb.servlet.CadastrarProduto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarProduto</servlet-name>
        <servlet-class>com.bmb.servlet.ListarProduto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EditarProduto</servlet-name>
        <servlet-class>com.bmb.servlet.EditarProduto</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CadastrarVenda</servlet-name>
        <servlet-class>com.bmb.servlet.CadastrarVenda</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>NewServlet</servlet-name>
        <url-pattern>/NewServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarTipo</servlet-name>
        <url-pattern>/CadastrarTipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarTipo</servlet-name>
        <url-pattern>/EditarTipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeletarTipo</servlet-name>
        <url-pattern>/DeletarTipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarTipo</servlet-name>
        <url-pattern>/ListarTipo</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarProduto</servlet-name>
        <url-pattern>/CadastrarProduto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarProduto</servlet-name>
        <url-pattern>/ListarProduto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EditarProduto</servlet-name>
        <url-pattern>/EditarProduto</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CadastrarVenda</servlet-name>
        <url-pattern>/CadastrarVenda</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
